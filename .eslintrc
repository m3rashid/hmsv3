{
  "env": {
    "es2021": true,
    "node": true,
    "browser": true
  },
  "extends": ["google", "prettier"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "new-cap": 0,
    "require-jsdoc": 0,
    "no-unused-vars": "warn",
    "valid-jsdoc": "off"
  }
}
